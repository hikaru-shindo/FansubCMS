# Ignore all the cache files
/application/data/cache/*

# Ignore sessions - they don't need to be versioned
/application/data/sessions/*

# Captchas don't need to be versioned
/public/media/common/images/tmp/*
!/public/media/common/images/tmp/delete.me

# We don't need to version uploaded files
/public/upload/*
!/application/resource/static/
/application/resource/static/*
!/application/resource/static/delete.me

# Only allow default modules to be versioned
/application/modules/*
!/application/modules/devtools/
!/application/modules/admin/
!/application/modules/cms/
!/application/modules/gadgets/
!/application/modules/group/
!/application/modules/install/
!/application/modules/news/
!/application/modules/projects/
!/application/modules/user/

# Ignore non-dist configs
/application/configs/database.ini
/application/configs/email.ini
/application/configs/environment.ini
/application/configs/navigation.ini

# ignore Frameworks in the library
/library/Doctrine*
/library/Zend/**

# Netbeans
/nbproject/*

# Eclipse project files
.classpath
.project
.buildpath
.settings/

# OS generated files
.DS_Store?
ehthumbs.db
Icon?
Thumbs.db

/vendor/
